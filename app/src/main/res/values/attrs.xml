<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundProgress">

        <attr name="ProgressColor" format="color"></attr>
        <attr name="ProgressBackgroundColor" format="color"></attr>
        <attr name="ProgressTextColor" format="color"></attr>
        <attr name="ProgressWidth" format="dimension"></attr>
        <attr name="ProgressTextSize" format="dimension"></attr>
        <attr name="ProgressMaxValue" format="integer"></attr>


    </declare-styleable>

    <declare-styleable name="DragBubbleView">
        <attr name="bubble_radius" format="dimension"/>
        <attr name="bubble_color" format="color"/>
        <attr name="bubble_text" format="string"/>
        <attr name="bubble_textSize" format="dimension"/>
        <attr name="bubble_textColor" format="color"/>
    </declare-styleable>

</resources>
